using System;

class MainClass
{
    public static string LetterChanges(string str)
    {
        
        char[] a=str.ToCharArray();
        foreach (char x in a)
        {
            Console.WriteLine("String {0}", x);
            Console.ReadLine();
        }

        
        string b = String.Empty;
        char c;


        foreach (char z in a)
        {
            if (z == 'z')
                c = 'a';
            else if (z == 'Z')
                c = 'A';
            else
                c = (char)((z) + 1);
        b += c;

        }
        return b;





        // code goes here  
        /* Note: In C# the return type of a function and the 
           parameter types being passed are defined, so this return 
           call must match the return type of the function.
           You are free to modify the return type. */
        //nextChar = (char)(((int)letter) + 1);


    }

    static void Main()
    {
        // keep this function call here
        Console.WriteLine("Type Your string:");
        string messedup = Console.ReadLine();
        Console.WriteLine("Your messed up string \n {0}", LetterChanges(messedup));
        Console.ReadLine();
    }

}



