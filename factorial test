using System;

class MainClass
{
    public static int FirstFactorial(int num)
    {
        Console.WriteLine("Good day you shit");
        string shit = Console.ReadLine();
        if (Int32.TryParse(shit, out num))
            Console.WriteLine("You have entered: {0}",num);
        Console.ReadLine();

        int factorial = num;

        for (int i = 1; i < num; i++)
        {
            factorial = factorial * i;
        }
        Console.WriteLine("{0}", factorial);
        Console.ReadLine();

        // code goes here  
        /* Note: In C# the return type of a function and the 
           parameter types being passed are defined, so this return 
           call must match the return type of the function.
           You are free to modify the return type. */

        return factorial;


    }

    static void Main()
    {
        // keep this function call here
        int finalAnswer = FirstFactorial(num);

        Console.WriteLine("Final answer:{0}",finalAnswer);
        Console.ReadLine();
    }
}
