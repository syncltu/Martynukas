using System;

class MainClass
{
    public static int FirstFactorial(int num)
    {
        if(num > 1)
        {
            return FirstFactorial(num-1) * num;
        }
        else
        {
            return num
        }
    }

    static void Main()
    {
        int pyder = Convert.ToInt32(Console.ReadLine("Good day you shit, type a number: "));
        Console.WriteLine("Factorial of number {1} is {0}", FirstFactorial(pyder), pyder);
        Console.ReadLine();
    }
}
