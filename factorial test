using System;

class MainClass
{
    public static int FirstFactorial(int num)
    {
        int factorial = num;
        for (int i = 1; i < num; i++)
        {
            factorial = factorial * i;
        }
        // code goes here  
        /* Note: In C# the return type of a function and the 
           parameter types being passed are defined, so this return 
           call must match the return type of the function.
           You are free to modify the return type. */
        return factorial;
    }

    static void Main()
    {
        // keep this function call here
        //int finalAnswer =FirstFactorial(num);
        Console.WriteLine("Good day you shit, type a number:");
        int pyder;
        pyder = Convert.ToInt32(Console.ReadLine());
        Console.WriteLine("Factorial of number {1} is {0}", FirstFactorial(pyder), pyder);
        Console.ReadLine();
    }
}
